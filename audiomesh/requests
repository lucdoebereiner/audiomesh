# -*- mode: restclient; url-max-redirections: 0 -*-
# Emacs restclient examples for httpbin.org
# http://httpbin.org/: HTTP Request & Response Service

# randomize graph
PUT http://localhost:8001/randomize

# get graph
GET http://localhost:8001/graph

# remove node
DELETE http://localhost:8001/node/0

# remove edge
DELETE http://localhost:8001/edge/10

